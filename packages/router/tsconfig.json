{
	"compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [ "ESNext", "WebWorker" ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "strict": true
  },
  "include": ["src"],
  "exclude": ["node_modules", ".dist", ".wrangler", ".cache"]
}
